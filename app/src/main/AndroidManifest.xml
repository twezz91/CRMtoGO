<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.crmtogo.deku.crmtogo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchCustomer" />
        <activity android:name=".createCustomer" />
        <activity android:name=".consultantEvents" />
        <activity
            android:name=".showCustomer"
            android:label="@string/title_activity_show_customer"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>